package com.nitin;

public class Test {

	Integer a = 1;
	int b = 2;
	
	public static void main(String args[]){
		@SuppressWarnings("unused")

		
		modify(a,b);
		System.out.println("%s" +a);
		System.out.println("%s" +b);
      
		
	}

	private static void modify(Integer a, int b) {
       a++;b++;
       System.out.println(a);
       System.out.println(b);
	}
}
